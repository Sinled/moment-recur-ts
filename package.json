    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-typescript": "3.0.9",
    "mocha": "4.1.0",
    "mocha-typescript": "1.1.12",
    "moment-timezone": "0.5.14",
    "nyc": "11.4.1",
    "prettier": "1.9.2",
    "prettier-webpack-plugin": "0.2.2",
